<!DOCTYPE html>
<html>
<head>
<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<style>

div{
	position: absolute;
	background-color: black;
}

html,body{

	height: 100%;
	width: 100%;
}
</style>
</head>
<body>

<script>
	$.getJSON("https://raw.githubusercontent.com/robbykraft/CoreInteraction/master/divs.json", function(data){
		for(var i = 0; i < data.length; i = i + 1){
			var div = document.createElement("div");
			div.style.top=data[i].top + "px";
			div.style.left=data[i].left + "px";
			div.style.width=data[i].width + "px";
			div.style.height=data[i].height + "px";
			document.body.appendChild(div)
		}
	});
</script>

</body>
</html>